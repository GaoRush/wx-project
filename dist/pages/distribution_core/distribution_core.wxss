page {
  background-color: #ededed;
  padding-bottom: 100rpx;
}

.distribution_core {
  display: flex;
  flex-direction: column;
  position: relative;
}

.distribution_core .head {
  display: flex;
  background-color: #4864ff;
  height: 400rpx;
  padding: 0 30rpx;
  padding-top: 64px;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 99;
  box-sizing: content-box;
  align-items: center;
}

.distribution_core .head .icon {
  position: absolute;
  top: 64px;
  right: 30rpx;
  width: 40rpx;
  height: 40rpx;
  margin-top: 40rpx;
}

.distribution_core_amount {
  text-align: center;
  color: #fff;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.distribution_core_amount .btn_getMoney {
  font-size: 24rpx;
  color: #4864ff;
  background-color: #fff;
  border-radius: 20px;
  padding: 10rpx 30rpx;
  margin-left: 20rpx;
  display: inline-block;
  text-align: center;
  line-height: 1;
}

.distribution_core_amount .btn_getWallet {
  font-size: 24rpx;
  color: #333;
  background-color: #fff;
  border-radius: 20px;
  padding: 20rpx 30rpx;
  display: inline-block;
  text-align: center;
  line-height: 1;
}

.distribution_core_amount .title {
  font-size: 24rpx;
  width: 100%;
}

.distribution_core_amount .amount {
  font-size: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  margin: 30rpx 0;
  line-height: 1;
}

.distribution_core_amount .amount_total {
  margin-bottom: 30rpx;
}

.distribution_core .main {
  padding: 30rpx;
  z-index: 9;
  padding-top: 64px;
  display: flex;
  flex-wrap: wrap;
  height: 100%;
}

.distribution_core .main .enter_item {
  width: 33.3%;
  height: 230rpx;
  text-align: center;
}

.distribution_core .main .enter_item .icon {
  width: 110rpx;
  height: 110rpx;
  margin-bottom: 10rpx;
}

.distribution_core .main .enter_item .title {
  font-size: 30rpx;
  color: #666;
  display: flex;
  flex-direction: column;
}

.distribution_core .main .enter_item .team_count {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 10rpx;
}

.distribution_core .main .enter_item .team_count .num {
  color: #ffb400;
}

.payTips_pop .btn_close {
  top: -100rpx;
}

.cash_withdrawal_main {
  padding: 0 30rpx;
}

.cash_withdrawal_main .amount_wrap {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.cash_withdrawal_main .amount_wrap .tle {
  font-size: 30rpx;
  border: 0;
}

.cash_withdrawal_main .amount_wrap .amount {
  font-size: 24rpx;
  color: #bababa;
}

.cash_withdrawal_main .use_money {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  border: 1px solid #ebebeb;
  margin: 20rpx 0;
  padding: 0 20rpx;
  border-radius: 4px;
  font-size: 28rpx;
}

.btn_caps {
  margin: 0 30rpx;
  margin-top: 30rpx;
}

.enter_item {
  position: relative;
}

/* pages/canvas/canvas.wxss */
canvas {
  position: fixed;
  top: 0;
  left: 9999px;
  opacity: 0;
}

.share {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: transparent;
  z-index: 99;
  opacity: 0;
}

.preview {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99999;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
  text-align: center;
}

.preview image {
  margin: 0 auto;
}

.preview .btn {
  position: fixed;
  bottom: 30rpx;
  left: 50%;
  background-color: #4864ff;
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: -125rpx;
  padding: 0;
  font-size: 30rpx;
}

.preview .icon_close {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  z-index: 99999;
  width: 68rpx;
  height: 68rpx;
}
