page {
  padding-bottom: 110rpx;
}

.goods_detail {
  width: 100%;
}

.goods_pic_wrap {
  position: relative;
  display: flex;
}

.goods_pic_wrap .tips {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 100%;
  text-align: center;
  color: #999;
  z-index: 99;
}

.goods_top .banner {
  background-color: #ededed;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 750rpx;
  position: relative;
  width: 100%;
}

.goods_top .banner .banner_pic {
  width: 100%;
  height: 750rpx;
}

.curNum {
  position: absolute;
  right: 30rpx;
  bottom: 30rpx;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  font-size: 30rpx;
  border-radius: 40rpx;
  text-align: center;
  padding: 10rpx 20rpx;
  line-height: 1;
  z-index: 99;
}

.goods_info_wrap {
  padding: 30rpx;
  font-size: 28rpx;
  color: #999;
}

.goods_info {
  line-height: 1.5;
}

.goods_info .goods_name {
  font-size: 34rpx;
  color: #333333;
  margin-bottom: 20rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-line-clamp: 3;
  -o-text-overflow: ellipsis;
  line-height: 1.2;
}

.goods_info .prd_price {
  display: flex;
  align-items: flex-end;
  color: #ffb400;
  font-size: 54rpx;
  margin: 20rpx 0;
  line-height: 1;
}

.btn_col {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 20rpx;
  color: #999;
}

.btn_col .icon {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}

.goods_stauts {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
}

.goods_content {
  padding: 30rpx;
}

.goods_content image {
  max-width: 100% !important;
  height: auto;
}

.goods_bottom .tips {
  text-align: center;
  margin: 30rpx;
}

.i_tle {
  position: relative;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
  height: 94rpx;
  line-height: 94rpx;
  padding: 0 50rpx;
}

.i_tle:after {
  display: block;
  content: '';
  width: 10rpx;
  height: 30rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  margin-top: -16rpx;
  background-color: #4864ff;
  border-radius: 20rpx;
}

.i_tle.boderTop {
  border-top: 20rpx solid #ededed;
}

.i_tle.boderBottom {
  border-bottom: 1rpx solid #ededed;
}

.bottom_tabBar {
  font-size: 30rpx;
  width: 100%;
  height: 110rpx;
  background-color: #fff;
  z-index: 9999;
  text-align: center;
  display: flex;
  align-items: center;
  box-shadow: 0 0 10rpx #c4c5c6;
  box-sizing: border-box;
  line-height: 1;
}

.bottom_tabBar.fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.bottom_tabBar .cell {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.bottom_tabBar .cell.br {
  border-right: 1px solid #ededed;
}

.bottom_tabBar .item {
  display: inline-block;
  padding: 10rpx 0;
  width: 100%;
  height: 100%;
  position: relative;
}

.bottom_tabBar .item.unRead .icon {
  position: relative;
  overflow: auto;
  padding-right: 20rpx;
  margin-right: -15rpx;
}

.bottom_tabBar .item.unRead .icon:after {
  content: '';
  position: absolute;
  right: 0;
  top: -5rpx;
  display: inline-block;
  width: 15rpx;
  height: 15rpx;
  background-color: #ffb400;
  border: 6rpx solid #fff;
  border-radius: 50%;
  z-index: 999;
}

.bottom_tabBar .item .icon {
  width: 48rpx;
  height: 48rpx;
  margin-bottom: 8rpx;
}

.bottom_tabBar .item .tle {
  display: block;
  font-size: 22rpx;
  color: #999;
}

.bottom_tabBar .btn {
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #4864ff;
  font-size: 30rpx;
}

.bottom_tabBar .btn.buy {
  color: #fff;
  background-color: #4864ff;
}

.gb {
  display: flex;
  flex-direction: column;
}

.gb .goods_info_wrap {
  padding: 0;
  display: flex;
  flex-direction: column;
}

.gb .end_time {
  font-size: 28rpx;
  color: #ffb400;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff3d6;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}

.gb .box {
  display: flex;
  margin: 0;
  width: 100%;
  padding: 30rpx;
}

.gb .goods_stauts {
  padding: 30rpx;
  padding-top: 0;
}

.gb .goods_stauts .num {
  margin-right: 6rpx;
  font-size: 24rpx;
  color: #ff6c00;
}

.gb .goods_stauts .num_wrap {
  display: flex;
  align-items: center;
}

.gb .gb_price_info {
  padding-bottom: 30rpx;
}

.gb .gb_price_info .i_tle {
  display: flex;
  align-items: center;
  justify-content: space-between;
  overflow: hidden;
  padding-right: 30rpx;
}

.gb .gb_price_info .i_tle .icon_wrap {
  display: flex;
  align-items: center;
}

.gb .gb_price_info .i_tle .icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 8rpx;
  opacity: .8;
}

.gb .gb_price_info .i_tle:after {
  background-color: #ffb400;
}

.gb .gb_price_info .item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  margin-top: 30rpx;
}

.gb .gb_price_info .prd_price {
  font-size: 40rpx;
  font-weight: normal;
}

.gb .gb_price_info .prd_price::before {
  font-size: 40rpx;
}

.gb .gb_price_info .gb_price {
  font-size: 30rpx;
  color: #999;
  text-decoration: line-through;
  font-weight: normal;
}

.gb .gb_price_info .gb_price::before {
  color: #999;
  font-size: 30rpx;
}

.prd_list {
  width: 100%;
  margin-bottom: 50rpx;
}

.i_tle {
  position: relative;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
  height: 94rpx;
  line-height: 94rpx;
  padding: 0 50rpx;
}

.i_tle:after {
  display: block;
  content: '';
  width: 10rpx;
  height: 30rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  margin-top: -16rpx;
  background-color: #4864ff;
  border-radius: 20rpx;
}

.i_tle.boderTop {
  border-top: 20rpx solid #ededed;
}

.i_tle.boderBottom {
  border-bottom: 1rpx solid #ededed;
}

.prd_list .prd_item {
  width: 100%;
  height: 196rpx;
  padding: 0 30rpx;
  float: none;
  position: relative;
  margin-bottom: 40rpx;
  margin-top: 30rpx;
  border-bottom: 0;
}

.prd_list .prd_item::after {
  content: '';
  display: inline-block;
  width: 67%;
  height: 0;
  position: absolute;
  right: 0;
  bottom: -20rpx;
  border-bottom: 1px solid #ededed;
}

.prd_list .prd_item:last-child {
  border-bottom: 0;
}

.prd_list .prd_item .prd_img {
  width: 196rpx;
  height: 196rpx;
  border: 1px solid #dbdbdb;
}

.prd_list .prd_item .prd_name {
  width: 100%;
  font-size: 30rpx;
  white-space: inherit;
  line-height: 1.5;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-line-clamp: 2;
  -o-text-overflow: ellipsis;
  margin-top: 0;
  color: #333;
}

.prd_list .prd_item .prd_price {
  color: #ffb400;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
  font-size: 32rpx;
  margin-top: 20rpx;
  font-weight: 600;
  display: flex;
  align-items: flex-end;
  line-height: 1;
  padding-bottom: 10rpx;
}

.prd_list .prd_item .prc_buyNum {
  width: 100%;
  font-size: 22rpx;
  color: #999;
}

.prd_list .prd_item .btn_shopCart {
  display: flex;
  margin: 20rpx 0;
}

.prd_list .prd_item .btn_shopCart .icon {
  width: 58rpx;
  height: 58rpx;
}

.prd_list .cell_hd {
  display: flex;
}

.prd_list .cell_bd {
  padding: 0 20rpx;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.prd_list .cell_ft {
  height: 100%;
  display: flex;
  align-items: flex-end;
}

.bottom_tabBar.gb {
  flex-direction: row;
}

.bottom_tabBar.gb .cell {
  width: 33.3%;
}

.bottom_tabBar.gb .cell.btn {
  width: 66.6%;
}

.gb .end_time.end {
  background-color: #999;
  color: #fff;
}
