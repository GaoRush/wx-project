page {
  background-color: #ededed;
  padding-bottom: 20rpx;
}

.team_list .team_item {
  width: 100%;
  height: auto;
  background-color: #fff;
  padding: 30rpx;
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  position: relative;
}

.team_list .team_item .icon {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.team_list .team_item .cell_hd {
  display: flex;
  align-items: center;
  height: 100%;
}

.team_list .team_item .cell_bd {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 0 30rpx;
}

.team_list .team_item .cell_bd .nick_name {
  width: 100%;
}
